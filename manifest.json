{
    "name": "Selecto",
    "version": "1.0",
    "description": "Copy URLs",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs",
        "clipboardWrite",
        "notifications"
    ],
    "background": {
        "scripts": ["src/background.js"],
        "persistent": false
    },
    "manifest_version": 2
}
